{
  "name": "juanmorales-resume-ui",
  "version": "0.1.0",
  "description": "My personal online resume",
  "main": "index.js",
  "scripts": {
    "prestart": "npm run clean-dist",
    "start": "npm-run-all --parallel server:dev lint:watch",
    "prebuild": "npm run clean-dist && npm run lint",
    "build": "npm run server:prod",
    "server:dev": "node fuse.dev.js",
    "server:prod": "node fuse.prod.js",
    "clean-dist": "npm run remove-dist && npm run remove-fuse-box-folder && mkdir dist && npm run copy:index",
    "remove-dist": "rimraf ./dist",
    "remove-fuse-box-folder": "rimraf ./.fusebox",
    "copy:index": "copyfiles -f src/index.html src/manifest.json dist",
    "lint": "esw src",
    "lint:watch": "npm run lint -- --watch"
  },
  "author": "Juan Morales <juank.memphis@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-polyfill": "6.23.0",
    "react": "15.4.2",
    "react-dom": "15.4.2",
    "react-redux": "5.0.2",
    "react-router": "3.0.2",
    "redux": "3.6.0",
    "redux-thunk": "2.2.0",
    "semantic-ui-css": "2.2.7",
    "semantic-ui-react": "0.65.0"
  },
  "devDependencies": {
    "babel-core": "6.23.1",
    "babel-eslint": "7.1.1",
    "babel-plugin-transform-react-constant-elements": "6.23.0",
    "babel-plugin-transform-react-jsx": "6.23.0",
    "babel-plugin-transform-react-remove-prop-types": "0.3.2",
    "babel-preset-latest": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-1": "6.22.0",
    "copyfiles": "1.2.0",
    "eslint": "3.16.0",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-react": "6.10.0",
    "eslint-watch": "3.0.0",
    "fuse-box": "1.3.117",
    "npm-run-all": "4.0.1",
    "redux-immutable-state-invariant": "1.2.4",
    "redux-logger": "2.8.1",
    "rimraf": "2.6.0",
    "uglify-js": "2.7.5"
  }
}
